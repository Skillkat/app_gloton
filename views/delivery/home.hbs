<div class="container mt-4">
  <h3>Mis Pedidos Asignados</h3>
  <div class="list-group">
    {{#each pedidos}}
    <a href="/delivery/pedido/{{this.id}}" class="list-group-item list-group-item-action">
      {{this.fecha}} - {{this.estado}} - {{this.comercio}} - ${{this.total}}
    </a>
    {{/each}}
  </div>
</div>
